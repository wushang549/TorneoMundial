{
    "version": 8,
    "configurePresets": [
        {
            "name": "clang",
            "displayName": "Clang 20.1.8 x86_64-redhat-linux-gnu",
            "description": "Using compilers: C = /usr/sbin/clang, CXX = /usr/sbin/clang++",
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/install/${presetName}",
                "CMAKE_C_COMPILER": "/usr/sbin/clang",
                "CMAKE_CXX_COMPILER": "/usr/sbin/clang++",
                "CMAKE_BUILD_TYPE": "Debug",
                "CMAKE_TOOLCHAIN_FILE": "/home/dnuno/workspace/container/cache/vcpkg/scripts/buildsystems/vcpkg.cmake"
            }
        }
    ]
}